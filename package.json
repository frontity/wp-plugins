{
  "name": "wp-org-plugins",
  "description": "A set of WordPress enchancement to improve the integration with the Frontity Framework",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.1.4",
    "fs-extra": "^8.1.0",
    "lerna": "^3.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "replace-in-file": "^3.4.3",
    "simple-git": "^1.107.0",
    "symlink-dir": "^3.1.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "scripts": {
    "dev": "webpack --watch",
    "sync": "node ./scripts/symlink.js",
    "sync:prod": "node ./scripts/symlink.js --production",
    "build": "node scripts/build.js",
    "version": "lerna exec --concurrency 1 -- node ../../scripts/version.js",
    "release": "lerna version --no-push --yes && npm run build"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@frontity/connect": "^1.0.1",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "minimist": "^1.2.0"
  }
}
